@using anonymous_forum_csharp.Models.ViewModels
@model HomeIndexViewModel
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <ul>
        @foreach (var item in Model.TopicList)
		{
            <li><a class="topics" href="@Url.Action("Index", "Thread", new {id = item.Id})">@item.Name</a></li>
		}
    </ul>
</div>
